import java.util.Scanner;
public class Main
{
   public static void main(String args[])
   {
      int number,count=0;
      Scanner sc = new Scanner(System.in);
      System.out.println("Enter a number :");
      number = sc.nextInt();
      for(int i = 2; i<=number; ++i) {
         while(number%i == 0)
         {
            number = number/i;
              count=count+1;
         }
         
      }
      System.out.println(count);
   }
}
input:
Enter a number :
225
output:
4
